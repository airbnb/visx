  "version": "0.0.189",
  "description": "vx point",
  "sideEffects": false,
  "main": "dist/vx-point.umd.js",
  "module": "dist/vx-point.es.js",
  "repository": "https://github.com/hshoff/vx",
  "files": [
    "dist",
    "build"
  ],
  "scripts": {
    "build": "npm run build:babel && npm run build:dist",
